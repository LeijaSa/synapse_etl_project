				"source": [
					"from pyspark.sql import SparkSession\n",
					"\n",
					"# Create Spark session\n",
					"spark = SparkSession.builder.appName(\"ExcelToCSV\").getOrCreate()\n",
					"\n",
					"# Path to Excel file in ADLS\n",
					"excel_path = \"abfss://library@group2projectstorage.dfs.core.windows.net/raw/library2015.xls\"\n",
					"\n",
					"# Read Excel file using Spark-Excel\n",
					"df = spark.read.format(\"com.crealytics.spark.excel\") \\\n",
					"    .option(\"header\", \"true\").option(\"inferSchema\", \"true\").option(\"dataAddress\", \"'All'!A1\").load(excel_path)  # Load the Excel file\n",
					"\n",
					"display(df)\n",
					"\n",
					"# Drop the unwanted column\n",
					"df = df.drop(\"Lainaus\")\n",
					"df = df.drop(\"Talous\")\n",
					"\n",
					"# Show DataFrame schema and preview data\n",
					"df.printSchema()\n",
					"df.show(5)\n",
					"\n",
					"# Save as CSV in ADLS\n",
					"csv_output_path = \"abfss://library@group2projectstorage.dfs.core.windows.net/raw/library2015.csv\"\n",
					"df.write.mode(\"overwrite\").option(\"header\", \"true\").csv(csv_output_path)"
				],
				"execution_count": 1
			}
		]
